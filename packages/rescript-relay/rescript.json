{
  "name": "rescript-relay",
  "version": "0.1.0",
  "namespace": false,
  "jsx": {
    "mode": "automatic",
    "version": 4
  },
  "sources": [
    {
      "dir": "src",
      "subdirs": true
    },
    {
      "dir": "__tests__",
      "type": "dev",
      "subdirs": true
    },
    {
      "dir": "__tests_preloaded__",
      "type": "dev",
      "subdirs": true
    }
  ],
  "package-specs": {
    "module": "commonjs",
    "in-source": true
  },
  "compiler-flags": [
    "-ppx ../../rescript-relay-ppx/_build/default/bin/RescriptRelayPpxApp.exe"
  ],
  "dependencies": ["@rescript/react"],
  "dev-dependencies": ["@glennsl/rescript-fetch"],
  "suffix": ".bs.js"
}
