{
  "name": "rescript-relay",
  "version": "0.1.0",
  "namespace": false,
  "jsx": {
    "mode": "automatic",
    "version": 4
  },
  "sources": [
    {
      "dir": "src",
      "subdirs": true
    },
    {
      "dir": "__tests__",
      "type": "dev",
      "subdirs": true
    }
  ],
  "package-specs": {
    "module": "commonjs",
    "in-source": true
  },
  "ppx-flags": [
    "./rescript-relay-ppx/_build/default/bin/RescriptRelayPpxApp.exe"
  ],
  "bs-dependencies": ["@rescript/react"],
  "bs-dev-dependencies": ["bs-fetch"],
  "suffix": ".bs.js",
  "warnings": {
    "error": "+A+101"
  }
}
