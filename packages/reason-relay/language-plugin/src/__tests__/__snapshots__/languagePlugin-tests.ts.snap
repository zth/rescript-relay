  let getConnectionNodes:
    option(fragment_participantsConnection) =>
    array(fragment_participantsConnection_edges_node) =
    connection =>
      switch (connection) {
      | None => [||]
      | Some(connection) =>
        switch (connection.edges) {
        | None => [||]
