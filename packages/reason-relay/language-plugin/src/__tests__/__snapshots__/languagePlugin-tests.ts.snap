    \\"fragment_participantsConnection_edges_node\\": unwrap_fragment_participantsConnection_edges_node,
  };
  let convertFragment = v =>
    v
    ->ReasonRelay.convertObj(
        fragmentConverter,
        fragmentConverterMap,
        Js.undefined,
      );
